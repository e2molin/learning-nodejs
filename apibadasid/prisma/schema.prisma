generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["cartografia", "public"]
}

model gasolineras {
  id                     Int     @id @default(autoincrement())
  ideess                 Int?
  rotulo                 String? @db.VarChar
  horario                String? @db.VarChar
  municipio              String? @db.VarChar
  provincia              String? @db.VarChar
  lat                    Float?
  lon                    Float?
  gasoleo_a              Float?
  gasoleo_b              Float?
  gasoleo_premium        Float?
  gasolina_95_e10        Float?
  gasolina_95_e5         Float?
  gasolina_95_e5_premium Float?
  gasolina_98_e10        Float?
  gasolina_98_e5         Float?

  @@schema("cartografia")
}

model spatial_ref_sys {
  srid      Int     @id
  auth_name String? @db.VarChar(256)
  auth_srid Int?
  srtext    String? @db.VarChar(2048)
  proj4text String? @db.VarChar(2048)

  @@schema("public")
}
